    "clsx": "^2.1.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "lucide-svelte": "^0.468.0",
    "mode-watcher": "^0.5.0",
    "postcss": "^8.4.49",
